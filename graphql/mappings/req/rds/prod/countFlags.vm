## If you modify where clause, please make sure you have modified the query in "listFlags.vm", too
{
  "version": "2018-05-29",
  "statements": [
    "Select * from Flags Where Where (IsNull(:searchText, '')='' Or (category Like Concat('%', :searchText, '%') Or type Like Concat('%', :searchText, '%'))) And (:status='' Or status=:status)"
  ],
  "variableMap": {
    ":searchText": "$ctx.args.searchText",
    ":status": "$ctx.args.status"
  }
}
